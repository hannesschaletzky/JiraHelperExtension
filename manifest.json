{
    "name": "Jira Helper",
    "description": "Automatically filters keywords you specified after switching to backlog and board",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
        },
    "action": {
        "default_title": "Click to view",
        "default_popup": "popup.html"
    }
}